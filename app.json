{
  "expo": {
    "name": "Hydrate",
    "slug": "hydrate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "scheme": "hydrate",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A1628"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.adamkaminski.hydrate",
      "infoPlist": {
        "NSUserTrackingUsageDescription": "This allows us to improve your experience with personalized content.",
        "NSNotificationUsageDescription": "We send reminders to help you stay hydrated throughout the day.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "buildNumber": "7"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A1628"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "9039b84e-b07a-4f7f-8d13-e3d0488cd4bb"
      }
    }
  }
}
